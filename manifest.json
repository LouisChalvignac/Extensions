{
  "name": "Cleaner Extension",
  "version": "1.0",
  "manifest_version": 3,

  "action": {
    "default_popup": "hello.html"
  },

  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ]
}
