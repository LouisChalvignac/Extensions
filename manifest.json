{
  "name": "Cleaner Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.jpg"
  },
  "permissions":[
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style-extension.css"],
      "js": ["content.js"]
    }
  ],
  "background":{
    "service_worker": "background.js"
  }
    
}